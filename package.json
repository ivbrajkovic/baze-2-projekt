{
  "name": "pivovara",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "if [[ ${NODE_ENV} == \"production\" ]]; then npm run start:prod; else npm run start:dev; fi",
    "start:dev": "DEBUG=pivovara:* nodemon -L ./bin/www",
    "start:prod": "NODE_ENV=production node ./bin/www"
  },
  "nodemonConfig": {
    "watch": [
      ".",
      "bin",
      "routes",
      "views"
    ],
    "delay": "1000"
  },
  "dependencies": {
    "colors": "^1.3.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "pg-promise": "^8.5.2",
    "pug": "2.0.0-beta11"
  }
}
